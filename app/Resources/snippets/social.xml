<?xml version="1.0" ?>

<template xmlns="http://schemas.sulu.io/template/template"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://schemas.sulu.io/template/template http://schemas.sulu.io/template/template-1.0.xsd">
    <key>social</key>
    <meta>
        <title lang="en">Social links</title>
    </meta>
    <properties>
        <property name="title" type="text_line" mandatory="true">
            <meta>
                <title lang="en">Title</title>
            </meta>
            <tag name="sulu.node.name"/>
        </property>
        <property name="githubUrl" type="url" mandatory="true">
            <meta>
                <title lang="en">Github URL</title>
            </meta>
            <tag name="sulu.node.name"/>
        </property>
        <property name="linkedInUrl" type="url" mandatory="true">
            <meta>
                <title lang="en">LinkedIn URL</title>
            </meta>
        </property>
        <property name="twitterUrl" type="url" mandatory="true">
            <meta>
                <title lang="en">Twitter URL</title>
            </meta>
        </property>
    </properties>
</template>
